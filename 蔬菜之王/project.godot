; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="蔬菜之王"
config/tags=PackedStringArray("复刻")
run/main_scene="res://Main.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Game="*res://Global/Game.gd"
SoundManager="*res://addons/sound_manager/sound_manager.gd"

[debug]

gdscript/warnings/unused_variable=0
gdscript/warnings/unused_local_constant=0

[display]

window/size/viewport_width=400
window/size/viewport_height=500
window/size/always_on_top=true
window/size/window_width_override=800
window/size/window_height_override=1000
window/stretch/mode="canvas_items"
window/stretch/aspect="ignore"

[editor_plugins]

enabled=PackedStringArray("res://addons/sound_manager/plugin.cfg")

[rendering]

2d/snap/snap_2d_transforms_to_pixel=true
2d/snap/snap_2d_vertices_to_pixel=true
